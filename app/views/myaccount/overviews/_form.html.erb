   <form class="form-inline">
     <div class="form-group">
      <tr>
        <td><%= f.label :first_name %></td>
        <td>&nbsp;:&nbsp;</td>
        <td><%= f.text_field :first_name, :placeholder => "First Name", class: 'form-control' %></td>
      </tr>
      <tr>
        <td><%= f.label :last_name %></td>
        <td>&nbsp;:&nbsp;</td>
        <td><%= f.text_field :last_name, class: 'form-control' %></td>
      </tr>
      <tr>
        <td><%= f.label :email %></td>
        <td>&nbsp;:&nbsp;</td>
        <td><%= f.text_field :email, class: 'form-control' %></td>
      </tr>
     </div>
    </form>
<!--   </fieldset>
  </div> -->

<!--   <div id='myaccount-profile_form' class='clearfix' >
